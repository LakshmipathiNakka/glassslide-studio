{
  "templateId": "businessStrategy",
  "templateName": "Business Strategy",
  "palette": {
    "primary": "#0A2540",
    "accent": "#00BFA6",
    "light": "#EAEAEA",
    "white": "#FFFFFF"
  },
  "font": {
    "heading": "Poppins SemiBold",
    "body": "Inter Regular"
  },
  "slides": [
    {
      "id": "cover",
      "name": "Cover",
      "items": [
        {
          "type": "image",
          "props": {
            "editable": true,
            "position": "center",
            "maxHeight": "150px",
            "objectFit": "contain",
            "src": ""
          }
        },
        {
          "type": "text",
          "props": {
            "text": "Your Tagline Here",
            "fontSize": 36,
            "fontFamily": "Poppins",
            "color": "#00BFA6",
            "alignment": "center"
          }
        }
      ]
    },
    {
      "id": "agenda",
      "name": "Agenda",
      "items": [
        { "type": "text", "props": { "text": "Agenda", "fontSize": 42, "color": "#0A2540" } },
        {
          "type": "list",
          "props": {
            "items": ["Introduction", "Market Insights", "Financial Summary", "SWOT Analysis", "Roadmap"],
            "bulletColor": "#00BFA6",
            "fontSize": 20
          }
        }
      ]
    },
    {
      "id": "mission-vision",
      "name": "Mission & Vision",
      "layout": "two-column",
      "items": [
        { "type": "text", "props": { "text": "Mission", "fontSize": 32, "color": "#0A2540" } },
        { "type": "text", "props": { "text": "Our mission is to drive innovation and deliver excellence.", "fontSize": 18 } },
        { "type": "text", "props": { "text": "Vision", "fontSize": 32, "color": "#0A2540" } },
        { "type": "text", "props": { "text": "Our vision is to become a global leader in sustainable growth.", "fontSize": 18 } }
      ]
    },
    {
      "id": "market-overview",
      "name": "Market Overview",
      "items": [
        {
          "type": "chart",
          "props": {
            "chartType": "bar",
            "data": [
              { "label": "Q1", "value": 25 },
              { "label": "Q2", "value": 40 },
              { "label": "Q3", "value": 35 },
              { "label": "Q4", "value": 50 }
            ],
            "accentColor": "#00BFA6",
            "grid": false
          }
        }
      ]
    },
    {
      "id": "financial-summary",
      "name": "Financial Summary",
      "items": [
        {
          "type": "table",
          "props": {
            "headers": ["Quarter", "Revenue", "Profit"],
            "rows": [
              ["Q1", "$1.2M", "$0.3M"],
              ["Q2", "$1.5M", "$0.5M"],
              ["Q3", "$1.8M", "$0.7M"],
              ["Q4", "$2.2M", "$0.9M"]
            ],
            "borderColor": "#00BFA6"
          }
        }
      ]
    },
    {
      "id": "swot",
      "name": "SWOT Analysis",
      "layout": "grid-2x2",
      "items": [
        { "type": "shape", "props": { "label": "Strengths", "fill": "#EAEAEA" } },
        { "type": "shape", "props": { "label": "Weaknesses", "fill": "#EAEAEA" } },
        { "type": "shape", "props": { "label": "Opportunities", "fill": "#EAEAEA" } },
        { "type": "shape", "props": { "label": "Threats", "fill": "#EAEAEA" } }
      ]
    },
    {
      "id": "team",
      "name": "Team Profiles",
      "items": [
        {
          "type": "profile",
          "props": {
            "members": [
              { "name": "John Smith", "role": "CEO", "image": "" },
              { "name": "Sara Lee", "role": "CFO", "image": "" },
              { "name": "Ravi Patel", "role": "CTO", "image": "" }
            ],
            "grid": "3x1"
          }
        }
      ]
    },
    {
      "id": "roadmap",
      "name": "Roadmap Timeline",
      "items": [
        {
          "type": "timeline",
          "props": {
            "milestones": [
              { "label": "Q1", "description": "Launch MVP" },
              { "label": "Q2", "description": "Expand to EU Market" },
              { "label": "Q3", "description": "New Product Line" },
              { "label": "Q4", "description": "IPO Readiness" }
            ],
            "color": "#00BFA6"
          }
        }
      ]
    },
    {
      "id": "kpi",
      "name": "KPI Dashboard",
      "items": [
        {
          "type": "metrics",
          "props": {
            "values": [
              { "icon": "target", "label": "Sales Growth", "value": "22%" },
              { "icon": "users", "label": "Customer Retention", "value": "89%" },
              { "icon": "chart-line", "label": "ROI", "value": "15%" },
              { "icon": "star", "label": "Brand Awareness", "value": "73%" }
            ]
          }
        },
        {
          "type": "chart",
          "props": {
            "chartType": "pie",
            "data": [
              { "label": "North America", "value": 40 },
              { "label": "Europe", "value": 30 },
              { "label": "Asia", "value": 30 }
            ],
            "accentColor": "#00BFA6"
          }
        }
      ]
    },
    {
      "id": "thank-you",
      "name": "Thank You",
      "items": [
        { "type": "text", "props": { "text": "Thank You!", "fontSize": 48, "color": "#FFFFFF", "alignment": "center" } },
        { "type": "text", "props": { "text": "contact@yourcompany.com", "fontSize": 18, "color": "#EAEAEA", "alignment": "center" } },
        { "type": "shape", "props": { "fill": "#0A2540", "stretch": "full" } }
      ]
    }
  ]
}
